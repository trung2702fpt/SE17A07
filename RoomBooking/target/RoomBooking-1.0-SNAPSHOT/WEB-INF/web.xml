<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>Controller.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetRooms</servlet-name>
        <servlet-class>Controller.GetRooms</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetEquipments</servlet-name>
        <servlet-class>Controller.GetEquipments</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetHistory</servlet-name>
        <servlet-class>Controller.GetHistory</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>Controller.NewServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetRooms</servlet-name>
        <url-pattern>/GetRooms</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetEquipments</servlet-name>
        <url-pattern>/GetEquipments</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetHistory</servlet-name>
        <url-pattern>/GetHistory</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/NewServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
