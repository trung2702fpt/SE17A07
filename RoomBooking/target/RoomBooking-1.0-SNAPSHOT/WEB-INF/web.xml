<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>Controller.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetRooms</servlet-name>
        <servlet-class>Controller.GetRooms</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetEquipments</servlet-name>
        <servlet-class>Controller.GetEquipments</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetHistory</servlet-name>
        <servlet-class>Controller.GetHistory</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>Controller.NewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchApi</servlet-name>
        <servlet-class>Controller.SearchApi</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>loginAdmin</servlet-name>
        <servlet-class>Controller.Admin.loginAdmin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>Controller.logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>sendMailServlet</servlet-name>
        <servlet-class>Controller.sendMailServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StaticAdmin</servlet-name>
        <servlet-class>Controller.StaticAdmin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditRoom</servlet-name>
        <servlet-class>Controller.Admin.EditRoom</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListRoom</servlet-name>
        <servlet-class>Controller.Admin.ListRoom</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchRoom</servlet-name>
        <servlet-class>Controller.Admin.SearchRoom</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetRooms</servlet-name>
        <url-pattern>/GetRooms</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetEquipments</servlet-name>
        <url-pattern>/GetEquipments</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetHistory</servlet-name>
        <url-pattern>/GetHistory</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/NewServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchApi</servlet-name>
        <url-pattern>/searchApi</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>loginAdmin</servlet-name>
        <url-pattern>/loginAdmin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>sendMailServlet</servlet-name>
        <url-pattern>/sendMailServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StaticAdmin</servlet-name>
        <url-pattern>/StaticAdmin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditRoom</servlet-name>
        <url-pattern>/EditRoom</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListRoom</servlet-name>
        <url-pattern>/ListRoom</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchRoom</servlet-name>
        <url-pattern>/SearchRoom</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <error-page>
        <error-code>404</error-code>
        <location>/error404.jsp</location>
    </error-page>
</web-app>
